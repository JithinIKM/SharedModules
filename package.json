{
  "name": "ksm-shared-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "eslint": "eslint src --ext .js,.jsx --max-warnings=0",
    "eslint:fix": "eslint src --ext .js,.jsx --fix",
    "dev": "vite --port 5002 --strictPort",
    "build": "vite build",
    "preview": "vite preview --port=5002 --strictPort",
    "prepare": "husky install",
    "mock:start": "cd mock && pnpm i && pnpm start",
    "mock:stop": "cd mock && pnpm delete"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "pnpm run eslint"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@ksmartikm/ui-components": "^0.1.18",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "i18next": "^23.5.1",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^13.2.2",
    "react-image-crop": "^10.1.8",
    "react-image-file-resizer": "^0.4.8",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vite-jsconfig-paths": "^2.0.1"
  }
}
